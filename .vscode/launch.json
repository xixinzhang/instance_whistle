    "version": "0.2.0",
    "configurations": [
        {
            "name": "Visualize results",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/mmdetection",
            "program": "tools/analysis_tools/analyze_results.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "../configs/mask-rcnn_r50-caffe_fpn_ms-3x_whistle.py",
                "work_dirs/mask-rcnn_r50-caffe_fpn_ms-poly-3x_coco/test.pkl",
                "work_dirs/mask-rcnn_r50-caffe_fpn_ms-poly-3x_coco/"
            ]
        },
        
        {
            "name": "Browse data",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/mmdetection",
            "program": "tools/analysis_tools/browse_dataset.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "../configs/mask-rcnn_r50-caffe_fpn_ms-3x_whistle.py",
                "--output-dir",
                "../outputs/mask_rcnn/",
                "--show-interval",
                "5"
            ],
        },
        {
            "name": "Mask RCNN Train",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/mmdetection",
            "program": "tools/train.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "../configs/mask-rcnn_r50-caffe_fpn_ms-3x_whistle.py",
            ],
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2"
            }
        },
        {
            "name": "DETR Train",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/mmdetection",
            "program": "tools/train.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "../configs/mask2former_r50_8xb2-lsj-50e_coco.py",
            ],
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2"
            }
        }
    ]
}